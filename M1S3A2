
let body = document.body;

let titulo = document.createElement('h1');
titulo.textContent = "Pagina dinamica";
body.appendChild(titulo);

let img = document.createElement("IMG");
img.src = "data:image/jpeg;base64,"
body.appendChild(img)

let img2 = document.createElement("IMG");
img2.src ="data:image/jpeg;base64,"
body.appendChild(img2)
img2.style.width = "4.75cm"


let img3 = document.createElement("IMG");
img3.src = "data:image/jpeg;base64,"
body.appendChild(img3)
img3.style.width = "4.75cm"

let texto = document.createElement('p');
texto.textContent = "Agora que entendemos parte da ligação entre o javascript e o navegador, podemos criar e remover ELEMENTOS HTML como bem entendermos";
body.appendChild(texto);
texto.style.textAlign = "center"

let ul = document.createElement('ul');
let li = document.createElement('li')
let lista = document.createTextNode("1")
let li2 = document.createElement('li')
let lista2 = document.createTextNode("2")
let li3 = document.createElement('li')
let lista3 = document.createTextNode("3")
li.appendChild(lista)
li2.appendChild(lista2)
li3.appendChild(lista3)
ul.appendChild(li)
ul.appendChild(li2)
ul.appendChild(li3)
body.appendChild(ul);



